<!-- Start Header -->
<%- include("./partials/header") %>
<!-- End Header -->

<!-- Start Container -->
<div class="content" style="margin: auto; width: auto;">
    <div class=" main same_price">
        <div class="main_header">
          <div class="container">
            <div class="searchbar">
              <input type="text" placeholder="Bạn muốn tìm khóa học nào ?" />
              <i class="fa fa-search search" aria-hidden="true"></i>
            </div>
          </div>
        </div>
        <div class="main_content">
          <div class="container ">
            <div class="row mb-4 d-flex justify-content-center">
              <div class="col-4">
                <ul>
                  <li>
                    <div class="courses_category"><a href="">Ngoại ngữ</a></div>
                  </li>
                  <li>
                    <div class="courses_category"><a href="">Thiết kế</a></div>
                  </li>
                  <li>
                    <div class="courses_category"><a href="">Bán hàng</a></div>
                  </li>
                  <li>
                    <div class="courses_category"><a href="">Phong cách sống</a></div>
                  </li>
                </ul>
              </div>
              <div class="col-4">
                <ul>
                  <li>
                    <div class="courses_category"><a href="">Marketing</a></div>
                  </li>
                  <li>
                    <div class="courses_category"><a href="">Kinh doanh và khởi nghiệp</a></div>
                  </li>
                  <li>
                    <div class="courses_category"><a href="">CNTT</a></div>
                  </li>
                  <li>
                    <div class="courses_category"><a href="">Nuôi dạy con</a></div>
                  </li>
                </ul>
              </div>
              <div class="col-4">
                <ul>
                  <li>
                    <div class="courses_category"><a href="">Tin học văn phòng</a></div>
                  </li>
                  <li>
                    <div class="courses_category"><a href="">Kỹ năng mềm</a></div>
                  </li>
                  <li>
                    <div class="courses_category"><a href="">Sức khỏe</a></div>
                  </li>
                  <li>
                    <div class="courses_category"><a href="">Hôn nhân</a></div>
                  </li>
                </ul>
              </div>
            </div>
            <div class="row">
              <div class="grid drag_to_scroll">
                <% courses.forEach(course=>{ %>
                  <div class="grid_items">
                    <a href="#">
                      <span class="sale-off"> -75%</span>
                      <img src="<%- course.img_src %> " alt="" />
                      <div class="course_content">
                        <h3 class="course_title"> <%- course.description %> </h3>
                        <div class="teacher_and_prevPrice">
                          <span class="teacher_name"><%- course.trainer_id %></span>
                          <span class="previous_price"><%- course.previous_price.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") %><sup>đ</sup></span>
                        </div>
                        <div class="stars_and_presentPrice">
                          <span class="star_rate">
                            <i class="fa fa-star co-or" aria-hidden="true"></i>
                            <i class="fa fa-star co-or" aria-hidden="true"></i>
                            <i class="fa fa-star co-or" aria-hidden="true"></i>
                            <i class="fa fa-star co-or" aria-hidden="true"></i>
                            <i class="fa fa-star co-or" aria-hidden="true"></i>
                            (7)
                          </span>
                          <span class="present_price"><%- course.present_price.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") %><sup>đ</sup></span>
                        </div>
                      </div>
                    </a>
                    <a href="/cart/add/<%- course._id %>">
                      <div class="d-flex justify-content-center"  >
                        <div class="btn btn-add-to-cart  mb-2">
                          <span class="d-block txt-add">Thêm vào giỏ hàng</span>
                          <span class="d-block txt-mem"> Đã có 847 người học</span>
                        </div>
                      </div>
                    </a>
                  </div>
                <% }) %>
